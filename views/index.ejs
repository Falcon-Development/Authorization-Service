  <script>
  function onSignIn(googleUser) {
    var profile = googleUser.getBasicProfile();
    var idToken = googleUser.getAuthResponse().id_token;//this is your token
    console.log("TOKEN", idToken)
  }
</script>
  </body>
</html>
